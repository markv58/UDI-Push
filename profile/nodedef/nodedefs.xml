<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller -->
    <nodeDef id="controller" nls="ctl">
        <editors />
        <sts>
			<st id="ST" editor="bool" />
		</sts>
        <cmds>
            <sends />
            <accepts>
              <cmd id="REMOVE_NOTICES_ALL" />
              <cmd id="UPDATE_PROFILE" />
            </accepts>
        </cmds>
    </nodeDef>
	<!-- thingnodetype -->
    <nodeDef id="thingnodetype" nls="thingnode">
        <editors />
        <sts>
        </sts>
        <cmds>
            <sends />
            <accepts>
		<cmd id="ACTIONS">
                  <p id="" editor="action" />
                </cmd>
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>
